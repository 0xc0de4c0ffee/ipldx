{
  "name": "ipfs-webui-cra",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:icons": "svgr -d src/icons node_modules/ipfs-css/icons && standard --fix src/icons/*",
    "lint": "standard",
    "test": "run-p --race test:serve test:run",
    "test:coverage": "run-p --race test:serve \"test:run -- --coverage\"",
    "test:serve": "ecstatic build --cache=0 --port=3001",
    "test:run": "react-scripts test",
    "analyze": "source-map-explorer build/static/js/main.*",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "cids": "^0.5.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "internal-nav-helper": "^1.0.2",
    "ipfs-css": "^0.3.1",
    "milliseconds": "^1.0.3",
    "multibase": "^0.4.0",
    "multicodec": "^0.2.6",
    "multihashes": "^0.4.13",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-debounce-render": "^4.0.1",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-test-renderer": "^16.3.2",
    "redux-bundler": "^21.2.0",
    "redux-bundler-react": "^1.0.0",
    "svgr": "^1.9.1",
    "tachyons": "^4.9.1",
    "window-or-global": "^1.0.1",
    "window.ipfs-fallback": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "ecstatic": "^3.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.1",
    "npm-run-all": "^4.1.2",
    "puppeteer": "^1.3.0",
    "source-map-explorer": "^1.5.0",
    "standard": "^11.0.1"
  },
  "homepage": "./",
  "standard": {
    "parser": "babel-eslint"
  }
}
